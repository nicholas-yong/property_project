         <%- include("controls/header") %>

         <script type = "text/javascript">
            $(function() 
            {
              let suburbNames = JSON.parse( '<%- JSON.stringify( data.suburbNames ) %>' );
              $("#Suburb").autocomplete({
                source: suburbNames,
                minLength: 2
              });
            });
         </script>
         <script type = "text/javascript">
        </script>
    </head>
        <body>
          
          <form>
            <div class = "suburb_input">
              <span> Suburb: </span>
              <input id = "Suburb" type = "text" aria-label= "Suburb Input">
              <button onclick = "test()"> Search </button>
            </div>
          </form>

          <table>
            <tr>
              <td> </td>
              <td> Address </td>
              <td> Land Shape </td>
              <td> Land Size </td>
              <td> Number of Bedrooms </td>
              <td> Number of Bathrooms </td>
              <td> Features </td>
              <td> Lat, Long </td>
              <td> Suburb </td>
              <td> On Market? </td>
            </tr>
          </table>

        </body>
</html>


